#include <stdio.h>

long int prime(int p);

int main(void)
{
	long int n = 20;
	int i, j;

	for (i = 2; i <= n/2; i++)
	{
		if (n % i == 0)
		{
			j = i;
/*   		printf("%d ", j);*/
			prime(j);
		}
	}

	return (0);
}

long int prime(int p)
{
        int i, flag = 0;

        for (i = 2; i <= p / 2; ++i)
        {
        /* if n is divisible by i, then n is not prime
           change flag to 1 for non-prime number*/
                if (p % i == 0)
                {
                        flag = 1;
                        break;
                }
        }
        /* flag is 0 for prime numbers */
        if (flag == 0)
                printf("%d\n", p);

        return (p);
}
